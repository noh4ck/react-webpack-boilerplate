{
  "presets": [["es2015"], "stage-2", "react", "react-boilerplate"],
  "plugins": [
    "react-hot-loader/babel",
    [
      "babel-plugin-dotenv",
      {
        "replacedModuleName": "babel-dotenv"
      }
    ],
    [
      "module-resolver",
      {
        "alias": {
          "@screens": "./app/screens",
          "@components": "./app/components",
          "@assets": "./app/assets"
        }
      }
    ]
  ]
}
